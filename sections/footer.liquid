<footer class="footer_grids">

  <div class="grid1" {% if section.settings.image %} style="background:url({{ section.settings.image | img_url :'master' }});background-size:cover;" {% endif %}>
<div class="dark">
    <h3><span>{{ section.settings.title }}</span></h3>
    <p>{{ section.settings.desc }}</p>
    {% render 'footer-newsletter' %}
  </div>
    </div>

  <div class="grid2">
<div class="parents_menu">
    <div class="menus">

      <h4>{{ section.settings.menu1_title }}</h4>
      <p>{{ section.settings.menu1_desc }}</p>
    </div>


        <div class="menus">

      <h4>{{ section.settings.menu2_title }}</h4>
      <p>{{ section.settings.menu2_desc }}</p>
    </div>

        <div class="menus">

      <h4>{{ section.settings.menu3_title }}</h4>
      <p>{{ section.settings.menu3_desc }}</p>
    </div>
  </div>
    <div class="social_grids">

      <ul class="no-bullets social-icons {{ additional_classes }}">

          <li>
      <a target="_blank" rel="noopener" href="{{ section.settings.fb }}" >
